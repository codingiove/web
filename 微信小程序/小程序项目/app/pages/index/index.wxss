.index_swiper swiper {
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;
}

.index_navigate {
  display: flex;
}

.index_navigate navigator {
  flex: 1;
  padding: 20rpx;
}

.index_navigate image {
  width: 100%;
}

.floor_title {
  padding: 10rpx 0;
}

.floor_title image {
  width: 100%;

}
.floor_list navigator {
  float: left;
  width: 33.33%;
}

/* 后4个img */
.floor_list navigator:nth-child(-n+4) image {
  height: 27.72vw;
  border-left: 10rpx solid #fff;
}

.floor_list navigator image {
  width: 100%;
  height: 100%;
}